<div class="container">
<h2>Create New Restaurant</h2>
<%= simple_form_for @restaurant do |f| %>
<%= f.input :name %>
<%= f.input :address %>
<%= f.input :phone_number %>
<%= f.input :category, collection: Restaurant::CATEGORY, as: :select %>
<%= f.submit class: "btn btn-primary" %>
<% end %>
</div>
